{
  "expo": {
    "name": "fixme",
    "slug": "fixme",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "plugins": ["@react-native-google-signin/google-signin"],
    "splash": {
      "image": "./assets/icons/fixmelogo5.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#FFFFFF"
      },
      "package": "com.khalid1995.fixme",
      "intentFilters": [
        {
          "action": "VIEW",
          "data": [
            {
              "scheme": "com.khalid1995.fixme",
              "host": "oauth2redirect"
            }
          ],
          "category": ["BROWSABLE", "DEFAULT"]
        }
      ]
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "scheme": "com.khalid1995.fixme",
    "extra": {
      "eas": {
        "projectId": "fdb303db-fd43-43c2-8319-3b88a7e45545"
      }
    }
  }
}
